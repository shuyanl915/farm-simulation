<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="model.inputC">
   <children>
      <Button fx:id="createAccount" layoutX="139.0" layoutY="304.0" mnemonicParsing="false" onAction="#create" text="Create Account" />
      <Button fx:id="clear" layoutX="316.0" layoutY="304.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="98.0" text="Clear" />
      <ComboBox fx:id="season" layoutX="251.0" layoutY="151.0" prefHeight="25.0" prefWidth="174.0" promptText="Choose Start Season" />
      <ComboBox fx:id="difficulty" layoutX="251.0" layoutY="200.0" prefHeight="25.0" prefWidth="174.0" promptText="Choose Game Difficulty" />
      <ComboBox fx:id="seedType" layoutX="251.0" layoutY="249.0" prefHeight="25.0" prefWidth="174.0" promptText="Choose Seed Type" />
      <TextField fx:id="nameF" layoutX="251.0" layoutY="104.0" onAction="#getInput" prefHeight="25.0" prefWidth="174.0" promptText="Enter Player Name" />
      <Label layoutX="103.0" layoutY="108.0" text="Player Name:" />
      <Label layoutX="103.0" layoutY="155.0" text="Season:" />
      <Label layoutX="103.0" layoutY="204.0" text="Difficulty:" />
      <Label layoutX="103.0" layoutY="253.0" text="Seed Type:" />
      <Button fx:id="start" layoutX="199.0" layoutY="56.0" mnemonicParsing="false" onAction="#startGame" prefHeight="25.0" prefWidth="133.0" text="Start Game" />
   </children>
</AnchorPane>
